  "dependencies": {
    "@frontity/core": "^1.7.3",
    "@frontity/google-analytics": "^1.2.0",
    "@frontity/hooks": "^2.0.2",
    "@frontity/html2react": "^1.3.4",
    "@frontity/tiny-router": "^1.2.1",
    "@frontity/wp-source": "^1.8.1",
    "@gfw/blog-theme": "./packages/gfw-blog",
    "axios": "^0.19.2",
    "btoa": "^1.2.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "emotion-rgba": "^0.0.8",
    "frontity": "^1.10.1",
    "gfw-components": "^1.3.10",
    "html-react-parser": "^0.13.0",
